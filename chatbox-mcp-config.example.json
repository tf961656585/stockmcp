{
  "mcpServers": {
    "alltick-stock-mcp": {
      "command": "npx",
      "args": [
        "--yes",
        "-p",
        "alltick-stock-mcp-server@latest",
        "stock-mcp-server"
      ],
      "env": {
        "ALLTICK_TOKEN": "your_alltick_token_here",
        "ALLTICK_RATE_MS": "11000",
        "JUHE_API_KEY": "your_juhe_api_key_here"
      }
    }
  }
}
