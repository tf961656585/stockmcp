{
  "mcpServers": {
    "alltick-stock-mcp": {
      "command": "powershell.exe",
      "args": [
        "-ExecutionPolicy",
        "Bypass",
        "-NoProfile",
        "-File",
        ".\\scripts\\start-mcp.ps1"
      ],
  "cwd": "D:/path/to/stockmcp",
      "env": {
        "ALLTICK_TOKEN": "your_alltick_token_here",
        "ALLTICK_RATE_MS": "11000",
        "JUHE_API_KEY": "your_juhe_api_key_here"
      }
    }
  }
}
